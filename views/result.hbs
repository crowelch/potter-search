{{#if noResults}}
	<h2 class="text-center">No Results Found</h2>
{{else}}
	<h2 class="text-center">Results for "{{query}}"</h2>
	<br>
	<p class="text-center">{{numResults}} results</p>
	<br>
	<ul>
		{{#each results}}
			<li>
				<ul>
					<li>
						<p>
							{{preText}}
							{{{text}}}
							{{postText}}
						</p>
					</li>
					<li>
						Book: {{book}}
					</li>
				</ul>
			</li>
			<br>
			<br>
		{{/each}}
	</ul>
{{/if}}
